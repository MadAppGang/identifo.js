{
  "compilerOptions": {
    "outDir": "dist",
    "module": "ES6",
    "target": "ES5",
    "lib": [
      "ESNext"
    ],
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "declaration": true,
    "strict": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "**/*.tsx",
    "**/*.ts",
    "**/*.js"
  ],
}