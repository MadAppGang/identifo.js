{
    "compilerOptions": {
      "module": "ES6",
      "target": "ES5",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": false,
      "outDir": "dist",
      "esModuleInterop": true,
      "declaration": true,
      "strict": true,
      "isolatedModules": true,
      "skipLibCheck": true,
    },
    "include": ["src", "**/*.tsx", "**/*.ts", "**/*.js"],
    "exclude": ["node_modules", "dist"]
}