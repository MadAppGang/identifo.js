{
  "compilerOptions": {
    "outDir": "dist",
    "module": "ES6",
    "target": "ES5",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "esModuleInterop": true,
    "declaration": true,
    "strict": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "include": ["src", "**/*.tsx", "**/*.ts", "**/*.js"],
}
